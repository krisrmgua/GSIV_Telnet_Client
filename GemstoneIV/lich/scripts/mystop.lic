mylist = Script.list
mylist.each do |i|
  if "#{i}" != "lnet" and "#{i}" != "infomon" and "#{i}" != "log" and "#{i}" != "quitall" and "#{i}" != "mystop" and "#{i}" != "myquit" and "#{i}" != "myquit2" and "#{i}" != "my_health_check"
    echo "Got #{i}"
    while running? "#{i}"
      echo "Killling #{i}"
      kill_script "#{i}"
    end 
  end
end
